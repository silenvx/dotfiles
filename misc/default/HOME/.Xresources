! 80桁24行の端末を表示
XTerm*geometry: 80x24
! utf8 encoding
XTerm*utf8: 1
! 多分utf8関連
XTerm*locale: true
! utf8のタイトル
XTerm*utf8Title: true
! altをmetaキーとして送信できるようにする
XTerm*metaSendsEscape: true
! 多分16bitのシーケンス制御に対応する
XTerm*wideChars: true
! 以下2行はよくわかっていない
XTerm*eightBitInput:false
XTerm*eightBitOutput:true
! Hyper_LにESC <F12>を割り当てる
XTerm*translations: #override <Key>Hyper_L: string("\033\033[24~")
! 記号などを全角で表示する (charmapで対応した方が良い)
!XTerm*cjkWidth: true
! 文字の幅を正しくする機能は不具合が出るのでoff
XTerm*forcePackedFont: true
! フォント (RictyZnはRictyに中国語フォントをマージさせたもの。幅が少しおかしいのでfonts.confで設定するかfaceNameにRictyなどの設定が必須)
XTerm*faceName: Ricty
! マルチバイト文字のフォント
!XTerm*faceNameDoublesize: Ricty
XTerm*faceNameDoublesize: Ricty
! xim
XTerm*ximFont: -misc-fixed-medium-r-normal--14-130-75-75-c-140-jisx0208.1983-0
! フォントのサイズ
XTerm*faceSize: 9
! $TERMのシェル変数
XTerm*xtermName: xterm-256color
! クリップボードの共有
XTerm*selectToClipboard: true
